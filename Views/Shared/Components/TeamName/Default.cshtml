@{

}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-teamName="" asp-route-pageNum="1" class="btn btn-outline-secondary btn-block">Home</a>
</div>

@foreach (string t in Model)
{
    <div class="d-grid gap-2">
        <a asp-controller="Home" asp-action="Index" asp-route-teamName="@t" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedTeamName == t ? "btn-primary" : "btn-outline-secondary") btn-block">@t</a>
    </div>
}


